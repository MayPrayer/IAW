<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mayprayer.system.mapper.SysCodeTypeMapper">

    <sql id="allColumn">
     id, create_time, create_by, update_time, update_by, is_delete, code_type_name, code_type_key, sort, status
    </sql>

    <resultMap type="SysCodeType" id="SysCodeTypeResult">
        <result property="id" column="id" />
        <result property="createTime" column="create_time" />
        <result property="createBy" column="create_by" />
        <result property="updateTime" column="update_time" />
        <result property="updateBy" column="update_by" />
        <result property="isDelete" column="is_delete" />
        <result property="codeTypeName" column="code_type_name" />
        <result property="codeTypeKey" column="code_type_key" />
        <result property="sort" column="sort" />
        <result property="status" column="status" />
    </resultMap>



</mapper>